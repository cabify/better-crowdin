{
  "name": "better-crowdin",
  "version": "1.0.0",
  "description": "Crowdin CLI on steroids",
  "skipBuild": true,
  "license": "UNLICENSED",
  "bin": {
    "better-crowdin": "./bin/better-crowdin"
  },
  "keywords": [
    "crowdin",
    "client",
    "translations",
    "i18n"
  ],
  "scripts": {
    "test": "cross-env BABEL_ENV=test jest",
    "test:watch": "cross-env BABEL_ENV=test jest --watch",
    "test:coverage": "cross-env BABEL_ENV=test jest --coverage",
    "test:ci": "cross-env BABEL_ENV=test jest --coverage --forceExit --maxWorkers=4 --no-watchman && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "babel-jest": "^23.0.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-es6-promise": "^1.1.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.23.0",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.2.1",
    "jest": "^23.1.0",
    "mock-fs": "^4.5.0",
    "prettier": "^1.13.4"
  },
  "dependencies": {
    "camelcase-keys": "^4.2.0",
    "chalk": "^2.4.0",
    "git-branch": "^2.0.1",
    "js-yaml": "^3.11.0",
    "json-query": "^2.2.2",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "yargs": "^11.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
